{
    "manifest_version": 2,
    "name": "Simple Filter Extension",
    "version": "1",
    "icons": {
        "48": "icons/48.jpg"
    },
    "description": "To filter out nasty words",
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["./src/script.js"]
        }
    ]
}